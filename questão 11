import java.util.Scanner;
public class Main {
  public static void main(String[] args){
    Scanner input = new Scanner(System.in);
    System.out.println("Digite um número: ");
    int a = input.nextInt();
    System.out.println("Digite outro número: ");
    int b = input.nextInt();
    int soma = 0;
    
    soma = a + b;
      System.out.println("A soma é: " + soma);
  }
}
   
